{
    "manifest_version": 3,
    "name": "Replace Bang",
    "version": "0.0.1",
    "description": "Substitutes defined Bangs with defined values.",
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [
        "*://www.google.com/*",
        "*://*.bing.com/*",
        "*://*.duckduckgo.com/*",
        "*://*.brave.com/*"
    ],
    "background": {
        "service_worker": "src/main.js"
    }
}